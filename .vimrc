filetype plugin indent on " Filetype-specific indentation
let g:netrw_banner = 0 " No banner
let g:netrw_liststyle = 1 " Long listing style
set noswapfile cursorline number relativenumber smartindent cindent laststatus=2 showtabline=2 " Options
syntax on " Some versions don't enable it by default
colorscheme elflord " Nice color scheme
